package com.hlzt.power.model;

import java.util.Date;

import com.hlzt.commons.model.BaseBean;

/**
 * 答辩记录
 * @author user
 *
 */
public class ReplyNote extends BaseBean{
	private String stuId;//学生ID	
	private String teaId;//指导老师ID
	private String teaStatus;//指导老师审核状态（0待审核  1通过  2驳回）
	private String leaderStatus;//专业负责人审核状态（0待审核 1通过  2驳回）
	private String replyNotePath;//答辩记录路径
	private Date limitTime;//限时
	
	
	public String getStuId() {
		return stuId;
	}
	public void setStuId(String stuId) {
		this.stuId = stuId;
	}
	public String getTeaId() {
		return teaId;
	}
	public void setTeaId(String teaId) {
		this.teaId = teaId;
	}
	public String getTeaStatus() {
		return teaStatus;
	}
	public void setTeaStatus(String teaStatus) {
		this.teaStatus = teaStatus;
	}
	public String getLeaderStatus() {
		return leaderStatus;
	}
	public void setLeaderStatus(String leaderStatus) {
		this.leaderStatus = leaderStatus;
	}
	public String getReplyNotePath() {
		return replyNotePath;
	}
	public void setReplyNotePath(String replyNotePath) {
		this.replyNotePath = replyNotePath;
	}
	public Date getLimitTime() {
		return limitTime;
	}
	public void setLimitTime(Date limitTime) {
		this.limitTime = limitTime;
	}
	
	
	
}
