package com.hlzt.power.model;

import java.util.Date;

import com.hlzt.commons.model.BaseBean;

/**
 * 申请导师
 * @author user
 *
 */
public class ApplyTeacher extends BaseBean{
	private String stuId;//学生ID
	private String stuName;//学生姓名
	private String teaId;//教师ID
	private String teaName;//教师姓名
	private String status;//审批状态（0待审核，1通过，2驳回）
	private Date limtTime;//限时
	
	public String getStuId() {
		return stuId;
	}
	public void setStuId(String stuId) {
		this.stuId = stuId;
	}
	public String getStuName() {
		return stuName;
	}
	public void setStuName(String stuName) {
		this.stuName = stuName;
	}
	public String getTeaId() {
		return teaId;
	}
	public void setTeaId(String teaId) {
		this.teaId = teaId;
	}
	public String getTeaName() {
		return teaName;
	}
	public void setTeaName(String teaName) {
		this.teaName = teaName;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public Date getLimtTime() {
		return limtTime;
	}
	public void setLimtTime(Date limtTime) {
		this.limtTime = limtTime;
	}
	
	
}
